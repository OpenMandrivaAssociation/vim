--- 550,559 ----
    if !has('job')
      return
    endif
+   " TODO: make this work for MS-Windows
+   if !has('unix')
+     return
+   endif
    call ch_log('Test_nl_write_out_file()')
    let job = job_start(s:python . " test_channel_pipe.py",
  	\ {'out-io': 'file', 'out-name': 'Xoutput'})
