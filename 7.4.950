      set_vim_var_nr(VV_HLSEARCH, 1L);
      set_vim_var_dict(VV_COMPLETED_ITEM, dict_alloc());
+     set_vim_var_list(VV_ERRORS, list_alloc());
      set_reg_var(0);  /* default for v:register is not 0 but '"' */
  
